    public static function getArrivalDates($origin, $destination, $departureDate, $departureTime, $arrivalTime)
    {
        $airport = (array) json_decode(self::$airport);
        $departureTemporary = \DateTime::createFromFormat("d-m-Y H:i:s", $departureDate . " " . $departureTime . ":00", new \DateTimeZone(self::$timezone[(string) intval($airport[$origin]->timeZone)]));
        $departure_dt = \DateTime::createFromFormat("d-m-Y H:i:s", $departureDate . " " . $departureTime . ":00", new \DateTimeZone(self::$timezone[(string) intval($airport[$origin]->timeZone)]))->setTimeZone(new \DateTimeZone("Asia/Jakarta"));
        $arrival_dt = \DateTime::createFromFormat("d-m-Y H:i:s", $departureDate . " " . $arrivalTime . ":00", new \DateTimeZone(self::$timezone[(string) intval($airport[$destination]->timeZone)]))->setTimeZone(new \DateTimeZone("Asia/Jakarta"));
        return (strtotime(gmdate('Y-m-d H:i:sO', $departure_dt->getTimestamp())) > strtotime(gmdate('Y-m-d H:i:sO', $arrival_dt->getTimestamp()))) ? $departureTemporary->modify("+24 hours")->format("d-m-Y") : $departureTemporary->format("d-m-Y");
    }

    public static function getDurations($origin, $destination, $departureTime, $arrivalTime)
    {
        $airport = (array) json_decode(self::$airport);
        $departureDate = date('d-m-Y');
        $departure_dt = \DateTime::createFromFormat("d-m-Y H:i:s", $departureDate . " " . $departureTime . ":00", new \DateTimeZone((string)  self::$timezone[(string) intval($airport[$origin]->timeZone)] ))->setTimeZone(new \DateTimeZone("Asia/Jakarta"));
        $arrival_dt = \DateTime::createFromFormat("d-m-Y H:i:s", $departureDate . " " . $arrivalTime . ":00", new \DateTimeZone(self::$timezone[(string) intval($airport[$destination]->timeZone)]))->setTimeZone(new \DateTimeZone("Asia/Jakarta"));
        $seconds = (strtotime(gmdate('Y-m-d H:i:sO', $departure_dt->getTimestamp())) > strtotime(gmdate('Y-m-d H:i:sO', $arrival_dt->getTimestamp()))) ? strtotime(gmdate('Y-m-d H:i:sO', \DateTime::createFromFormat("d-m-Y H:i:s", $departureDate . " " . $arrivalTime . ":00", new \DateTimeZone(self::$timezone[(string) intval($airport[$destination]->timeZone)]))->modify("+24 hours")->setTimeZone(new \DateTimeZone("Asia/Jakarta"))->getTimestamp())) - strtotime(gmdate('Y-m-d H:i:sO', $departure_dt->getTimestamp())) : strtotime(gmdate('Y-m-d H:i:sO', $arrival_dt->getTimestamp())) - strtotime(gmdate('Y-m-d H:i:sO', $departure_dt->getTimestamp()));
        return (int) ($seconds / 3600) . 'j' . (int) (($seconds - ((int) ($seconds / 3600)) * 3600) / 60).'m';
    }
    public static function getFormatYMD($datetime)
    {
    	//convert asterix format 13-Des-2011 11:20 to 2011-12-13 11:20 
    	$bulan = array(1=>'Jan','Feb','Mar','Apr','Mei','Jun','Jul','Agu','Sep','Okt','Nov','Des');
		list($date,$time) = explode(" ",$datetime);
		list($d,$m,$y) = explode("-",$date);
		$index = array_search($m, $bulan);
		$month = str_pad($index, 2, '0', STR_PAD_LEFT); 
		$ymdhis = $y.'-'.$month.'-'.$d.' '.$time;
		return $ymdhis;

    }
    
